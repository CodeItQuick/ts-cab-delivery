datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")

}

generator client {
  provider = "prisma-client-js"
}

model Cabs {
  id        Int     @id @default(autoincrement())
  CabName   String
  Status    String
  CabRevenue CabRevenue[]
}
model Customers {
  id             Int     @id @default(autoincrement())
  CustomerName   String
  Status         String
}
model CabRevenue {
  Id             Int     @id @default(autoincrement())
  Cab            Cabs?    @relation(fields: [CabId], references: [id])
  CabId          Int?
  Fare           Int     @default(0)
}