datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")

}

generator client {
  provider = "prisma-client-js"
}

model Cabs {
  id        Int     @id @default(autoincrement())
  CabName   String
  Status    String
  Revenue   BigInt @default(0)
}
model Customers {
  id             Int     @id @default(autoincrement())
  CustomerName   String
  Status         String
}