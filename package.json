{
  "name": "ts-cab-delivery",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc --watch -p .",
    "test:unit": "dotenv -e .env.test -- npx prisma migrate deploy && dotenv -e .env.test -- jest -c jest.config.ts",
    "test:unit:ui": "npx prisma migrate deploy && jest -c jest.config.ts --ui",
    "start": "tsx --env-file=.env.test src/index.ts",
    "migrate": "npx prisma migrate dev --name ",
    "resolve": "npx prisma migrate resolve --applied ",
    "generate": "npx prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "inquirer": "^12.3.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.18.0",
    "jest": "~29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0"
  }
