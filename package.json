{
  "name": "ts-cab-delivery",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc --watch -p .",
    "test:unit": "dotenv -e .env.test -- npx prisma migrate deploy && dotenv -e .env.test -- vitest -c ./vitest.config.unit.ts",
    "test:unit:ui": "npx prisma migrate deploy && vitest -c ./vitest.config.unit.ts --ui",
    "start": "node src/program.ts",
    "migrate": "npx prisma migrate dev --name ",
    "resolve": "npx prisma migrate resolve --applied ",
    "generate": "npx prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "inquirer": "^12.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^2.0.2"
  }
}
